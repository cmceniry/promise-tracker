  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose --maxWorkers=2",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "ignores": [
      "wptpkg/*"
    ],
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
