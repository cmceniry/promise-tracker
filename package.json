{
  "name": "promiseviewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^8.10.0",
    "bootstrap": "^5.1.3",
    "crypto-js": "^4.1.1",
    "js-yaml": "^4.1.0",
    "mermaid": "^8.13.10",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.1",
    "react-dom": "^17.0.2",
